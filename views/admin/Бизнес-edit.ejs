    <section class="companies-section">
      <div class="companies-container">
        <h2>Засах горим</h2>
        <div class="companies-box">
          <!-- Item start -->
            <div class="company-item">
              <div class="image" id="image" style="background-image: url('../../images/companies/<%= items[id].img%>');" ></div>
              <div class="text-box">
                <h3 class="u-gallery-heading" id="heading"><%= items[id].heading%></h3>
                <p class="u-gallery-text" id="paragraph"><%= items[id].paragraph%></p>
                <p id="href"><%= items[id].href%></p>
              </div>
            </div>
          <!-- Item End -->
        </div>
        <fieldset>
          <legend>Edit here</legend>
          <form action="/admin/business/<%= id %>/save?_csrf=<%= csrfToken %>" method="POST" enctype="multipart/form-data" name="form" id="form" class="edit-form">
            <input type="text" name="heading" onchange="onchange1(this)" value="<%= items[id].heading%>" >
            <input type="text" name="paragraph" onchange="onchange2(this)" value="<%= items[id].paragraph%>" >
            <input type="text" name="href" onchange="onchange3(this)" value="<%= items[id].href%>" >
            <input type="file" onchange="imageController(this)" accept="image/*" name="image">
            <input type="submit" value="Save">
          </form>
        </fieldset>
        
        
        <button onclick="create()" style="background-color: red; color: white; margin-top: 10px;">Delete</button>
        </div>
      </div>
    </section>
    
    
    
    <footer class="u-align-center u-clearfix u-footer u-white u-footer" id="sec-4623"><div class="u-align-left u-clearfix u-sheet u-valign-middle u-sheet-1">
