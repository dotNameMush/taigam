      </div></footer>
    <script>
      function onchange1(input) {
        document.getElementById('heading').innerText = input.value;
      }
      function onchange2(input) {
        document.getElementById('paragraph').innerText = input.value;
      }
      function onchange3(input) {
        document.getElementById('href').innerText = input.value;
      }
      function imageController(input) {
        if (input.files && input.files[0]) {
          var reader = new FileReader();
          reader.onload = function (e) {
            var style = "background-image: url('" + e.target.result + "');"
          document.getElementById("image").setAttribute('style', style)
          };
          reader.readAsDataURL(input.files[0]);
        }
      }    
    </script>
  
