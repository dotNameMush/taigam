  <script>
    function saveTextInfo() {
          function setback() {
            document.getElementById('savebutton').innerText = 'Save';
          }
          const timeout = setTimeout(setback, 2000);
          
            var formData = {
                s1h: document.getElementById('s1h').innerText,
                s1p1: document.getElementById('s1p1').innerText,
                s1p2: document.getElementById('s1p2').innerText,
                s2h: document.getElementById('s2h').innerText,
                s2p1: document.getElementById('s2p1').innerText,
                s2p2: document.getElementById('s2p2').innerText,
                s2p3: document.getElementById('s2p3').innerText,
                
            }
          fetch("/admin/about/update-text", {
          method: "POST",
          headers: {
            Accept: "application/json",
            "Content-Type": "application/json",
            "CSRF-Token": Cookies.get("XSRF-TOKEN"),

          },
          body: JSON.stringify(formData),
        }).then(function(response){
          switch (response.status) {
            case 201:
              document.getElementById('savebutton').innerText = 'Амжилттай';
              timeout
              break;
            
            case 500:
              document.getElementById('savebutton').innerText = 'Алдаа гарлаа';
              timeout
              break;
          }
          console.log(response.status)
          
        })
      }
  </script>
