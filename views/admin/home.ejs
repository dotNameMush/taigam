  <script>
    function saveTextInfo() {
          function setback() {
            document.getElementById('savebutton').innerText = 'Save';
          }
          const timeout = setTimeout(setback, 2000);
          
            var formData = {
                s1h: document.getElementById('s1h').innerText,
                s1p: document.getElementById('s1p').innerText,
                s2h: document.getElementById('s2h').innerText,
                s2p: document.getElementById('s2p').innerText,
                service1h: document.getElementById('service1h').innerText,
                service1p: document.getElementById('service1p').innerText,
                service2h: document.getElementById('service2h').innerText,
                service2p: document.getElementById('service2p').innerText,
                service3h: document.getElementById('service3h').innerText,
                service3p: document.getElementById('service3p').innerText,
                service4h: document.getElementById('service4h').innerText,
                service4p: document.getElementById('service4p').innerText,
                showcaseh: document.getElementById('showcaseh').innerText,
                showcasep: document.getElementById('showcasep').innerText,
                abouth: document.getElementById('abouth').innerText,
                aboutp: document.getElementById('aboutp').innerText,
                careerh: document.getElementById('careerh').innerText,
                careerp: document.getElementById('careerp').innerText 
            }
          fetch("/admin/home/update-text", {
          method: "POST",
          headers: {
            Accept: "application/json",
            "Content-Type": "application/json",
            "CSRF-Token": Cookies.get("XSRF-TOKEN"),

          },
          body: JSON.stringify(formData),
        }).then(function(response){
          switch (response.status) {
            case 201:
              document.getElementById('savebutton').innerText = 'Амжилттай';
              timeout
              break;
            
            case 500:
              document.getElementById('savebutton').innerText = 'Алдаа гарлаа';
              timeout
              break;
          }
          console.log(response.status)
          
        })
      }
    function slider1(input) {
        if (input.files && input.files[0]) {
          var reader = new FileReader();
          reader.onload = function (e) {
            var style = "background-image: linear-gradient(0deg, rgba(0,0,0,0.4), rgba(0,0,0,0.4)), url('" + e.target.result + "');"
          document.getElementById("slider1").setAttribute('style', style)
          };
          reader.readAsDataURL(input.files[0]);
        }
      }    
      function slider2(input) {
        if (input.files && input.files[0]) {
          var reader = new FileReader();
          reader.onload = function (e) {
            var style = "background-image: linear-gradient(0deg, rgba(0,0,0,0.4), rgba(0,0,0,0.4)), url('" + e.target.result + "');"
          document.getElementById("slider2").setAttribute('style', style)
          };
          reader.readAsDataURL(input.files[0]);
        }
      }
      function imageController(input) {
        if (input.files && input.files[0]) {
          var reader1 = new FileReader();
                var reader2 = new FileReader();
                var reader3 = new FileReader();
                var reader4 = new FileReader();

                reader1.onload = function (e) {
                    $('#showcasei0')
